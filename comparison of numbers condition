/*Write a C program to read the value of an integer m and display the value of n is 1 when m is larger than 0, 0 when m is 0 and -1 when m is less than 0*/

#include<stdio.h>
int main()
{
    int val,n;
    printf("enter number:");
    scanf("%d",&val);
    if(val!=0)
        if(val>0)
            n=1;
    else
       n=-1;
     else
       n=0;
    printf("the element is %d",val);
    printf("\nthe value is %d",n);
    
    return 0;
    
}
